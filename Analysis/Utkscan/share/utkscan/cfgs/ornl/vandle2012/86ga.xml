    <Description>LeRIBSS 2012, 3Hen hybrid, 2 Clovers</Description>

    <Global>
        <Revision version="F"/>
        <EventWidth unit="s" value="50e-6"/>
        <EnergyContraction value="1.0"/>
        <Path>config/</Path>
