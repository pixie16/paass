    <Description>
        LeRIBSS 2011 73Cu experiment, 4 Clovers, 2 Beta scitilators, MTC
    </Description>

    <Global>
        <Revision version="D"/>
        <EventWidth unit="s" value="1e-6"/>
        <EnergyContraction value="4.0"/>
        <Path>config/</Path>
