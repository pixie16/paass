    <DetectorDriver>
        <Processor name="GeProcessor"
            gamma_threshold="20.0"
            low_ratio="1.5"
            high_ratio="3.0"
            sub_event="100e-9"
            gamma_beta_limit="100e-9"
            gamma_gamma_limit="200e-9"
            cycle_gate1_min="1.0"
            cycle_gate1_max="1.5"
            cycle_gate2_min="1.5"
            cycle_gate2_max="2.0"
        />
        <Processor name="MtcProcessor"
            double_stop="True"
            double_start="True"/>
        <Processor name="BetaScintProcessor"
                   gamma_beta_limit="200e-9"
                   energy_contraction="2"/>
    </DetectorDriver>

