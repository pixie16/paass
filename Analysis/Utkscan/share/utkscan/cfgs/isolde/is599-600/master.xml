               low_ratio="1.5"
               high_ratio="3.0"
               sub_event="1e-6"
               gamma_beta_limit="1e-6"
               gamma_gamma_limit="2e-6"
               cycle_gate1_min="1.0"
               cycle_gate1_max="1.5"
               cycle_gate2_min="1.5"
               cycle_gate2_max="2.0" />
    <Processor name="DoubleBetaProcessor"/>
    <Processor name="VandleProcessor"
               types="small,medium" res="2" offset="1000" NumStarts="1"/>
    <Processor name="IS600Processor"/>
  </DetectorDriver>

